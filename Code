# Web-Scraper-Code
Beautiful Soupness

import urllib2
from bs4 import BeautifulSoup
from splinter import Browser



walmart_url = "https://jobs.walmart.com/us/jobs?page=1"


with Browser() as browser:
   print "test"
#   Visit (URL)
   browser.visit(walmart_url)
   html_source = browser.html
   
   soup = BeautifulSoup(html_source, 'lxml')
   
   
#   for link in soup.find_all(''):
#      print link

#   table = soup.find_all("table", class_="job-listings")
#   print table
   
#   table = soup.find(lambda tag: tag.name=='table' and tag.has_key('id') and tag['id']=="job-listings") 
#   rows = soup.findAll(lambda tag: tag.name=='tr')

   table = soup.find("table", { "class" : "job-listings" })
   table_body = table.find('tbody')

   rows = table_body.find_all('tr')
   for row in table.findAll('tr'):
       tds = row('td')
